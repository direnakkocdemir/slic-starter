{
  "name": "slic-starter",
  "version": "1.0.0",
  "description": "A complete serverless starter application",
  "scripts": {
    "coverage": "npm run test -- --cov --coverage-report=html",
    "format": "eslint --cache --fix .",
    "test:lint": "eslint .",
    "test:packages": "tap \"*/test/**/*.test.js\" --branches=100 --statements=100 --functions=100 --lines=100",
    "test": "npm run test:lint && npm run test:packages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fourTheorem/slic-starter.git"
  },
  "keywords": [
    "serverless",
    "starter",
    "project",
    "aws"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fourTheorem/slic-starter/issues"
  },
  "homepage": "https://github.com/fourTheorem/slic-starter#readme",
  "devDependencies": {
    "aws-sdk-mock": "^4.3.0",
    "awscred": "^1.4.2",
    "eslint": "^7.26.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.23.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.0.0",
    "proxyquire": "^2.1.3",
    "serverless": "^2.60.0",
    "tap": "^15.0.10"
  },
  "dependencies": {
    "@hapi/bourne": "^2.0.0",
    "aws-sdk": "^2.867.0",
    "aws-signed-axios": "^1.1.0",
    "aws-xray-sdk": "^3.2.0",
    "lambda-logger-middleware": "^1.0.3",
    "lodash": "^4.17.21",
    "middy": "^0.36.0",
    "middy-autoproxyresponse": "^0.1.0",
    "pino": "^6.11.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix"
    ]
  }
}
